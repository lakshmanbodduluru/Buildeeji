<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/settings_share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Share"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/settings_rate_us"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Rate Us"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/settings_about_us"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="About us"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/settings_contact_us"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Contact us"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/settings_feed_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Feed back"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/settings_help_faq"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="@string/help_faq"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/settings_live_chat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Live chat"
        android:textColor="@color/black"
        android:textSize="20sp" />


</LinearLayout>
